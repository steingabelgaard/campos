        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <page name="sales_purchases" position="after">
            	<page name="campos" string="CampOS" >
            		<group>
            			<field name="scoutgroup" />
            			<field name="participant" />
